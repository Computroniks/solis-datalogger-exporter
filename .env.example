# Example configuration file. 

# IP of your datalogging stick
DATALOGGER_IP=192.168.1.10

# Username used to access the web interface
DATALOGGER_UNAME=admin

# Password used to access the web interface
DATALOGGER_PWD=admin

# Number of retries to attempt before assuming the stick is down
DATALOGGER_RETRIES=2

# Timeout for connections to stick
DATALOGGER_TIMEOUT=2

# Port the exporter should listen on
EXPORTER_PORT=9100